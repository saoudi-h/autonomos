{
    "name": "@autonomos/cli",
    "version": "0.0.1",
    "description": "CLI tool for the Autonomos Agent Protocol",
    "type": "module",
    "bin": {
        "autonomos": "./dist/index.mjs"
    },
    "scripts": {
        "start": "node dist/index.mjs",
        "build": "tsdown && cp ../../LICENSE .",
        "lint": "eslint . --max-warnings 0",
        "lint:fix": "eslint . --max-warnings 0 --fix",
        "format:check": "prettier \"**/*\" --ignore-unknown --list-different",
        "format:fix": "prettier \"**/*\" --ignore-unknown --write",
        "clean": "rimraf node_modules dist LICENSE",
        "typecheck": "tsc -p tsconfig.json --noEmit",
        "test": "vitest",
        "test:ui": "vitest --ui",
        "test:run": "vitest run",
        "test:coverage": "vitest run --coverage",
        "prepublishOnly": "pnpm run build",
        "pre-commit": "lint-staged"
    },
    "keywords": [
        "cli",
        "agent",
        "protocol",
        "ai",
        "workflow",
        "ai-agent",
        "autonomos"
    ],
    "author": "Autonomos",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/saoudi-h/autonomos.git",
        "directory": "packages/cli"
    },
    "bugs": {
        "url": "https://github.com/saoudi-h/autonomos/issues"
    },
    "homepage": "https://github.com/saoudi-h/autonomos/tree/main/packages/cli#readme",
    "publishConfig": {
        "access": "public"
    },
    "files": [
        "dist",
        "README.md",
        "LICENSE"
    ],
    "dependencies": {
        "@autonomos/core": "workspace:*",
        "commander": "^14.0.2",
        "picocolors": "1.1.1"
    },
    "devDependencies": {
        "@repo/eslint": "workspace:*",
        "@repo/prettier": "workspace:*",
        "@repo/tsdown": "workspace:*",
        "@types/node": "^25.0.3",
        "typescript": "^5.0.0"
    },
    "main": "./dist/index.mjs",
    "module": "./dist/index.mjs",
    "types": "./dist/index.d.mts",
    "exports": {
        ".": "./dist/index.mjs",
        "./package.json": "./package.json"
    }
}
