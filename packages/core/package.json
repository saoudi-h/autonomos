{
    "name": "@autonomos/core",
    "version": "0.0.2",
    "description": "SDK and core definitions for the Agent Protocol",
    "type": "module",
    "main": "./dist/index.mjs",
    "types": "./dist/index.d.mts",
    "scripts": {
        "dev": "tsdown --watch",
        "build": "tsdown && cp ../../LICENSE .",
        "lint": "eslint . --max-warnings 0",
        "lint:fix": "eslint . --max-warnings 0 --fix",
        "format:check": "prettier \"**/*\" --ignore-unknown --list-different",
        "format:fix": "prettier \"**/*\" --ignore-unknown --write",
        "clean": "rimraf node_modules dist LICENSE",
        "typecheck": "tsc -p tsconfig.json --noEmit",
        "test": "vitest",
        "test:ui": "vitest --ui",
        "test:run": "vitest run",
        "test:coverage": "vitest run --coverage",
        "prepublishOnly": "pnpm run build",
        "pre-commit": "lint-staged"
    },
    "keywords": [
        "agent",
        "protocol",
        "ai",
        "workflow",
        "ai-agent",
        "mcp",
        "autonomy"
    ],
    "author": "Autonomos",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/saoudi-h/autonomos.git",
        "directory": "packages/core"
    },
    "bugs": {
        "url": "https://github.com/saoudi-h/autonomos/issues"
    },
    "homepage": "https://github.com/saoudi-h/autonomos/tree/main/packages/core#readme",
    "publishConfig": {
        "access": "public"
    },
    "files": [
        "dist",
        "README.md",
        "LICENSE"
    ],
    "devDependencies": {
        "@repo/eslint": "workspace:*",
        "@repo/prettier": "workspace:*",
        "@repo/tsdown": "workspace:*",
        "@repo/vitest": "workspace:*",
        "@types/js-yaml": "4.0.9",
        "typescript": "^5.0.0"
    },
    "module": "./dist/index.mjs",
    "exports": {
        ".": "./dist/index.mjs",
        "./package.json": "./package.json"
    },
    "dependencies": {
        "js-yaml": "4.1.0",
        "zod": "3.24.1"
    }
}
